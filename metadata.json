{
  "name": "blackknight36-bacula",
  "version": "2.0.12",
  "author": "Michael Watters <wattersm@watters.ws>",
  "summary": "Installs and manages the bacula backup infrastructure.",
  "license": "Apache-2.0",
  "source": "https://github.com/blackknight36/puppet-bacula.git",
  "project_page": "https://github.com/blackknight36/puppet-bacula",
  "issues_url": "https://github.com/blackknight36/puppet-bacula/issues",
  "description": "This module manages Bacula, a complete backup solution.",
  "tags": ["bacula", "backup"],
  "operatingsystem_support": [
    { "operatingsystem":"RedHat", "operatingsystemrelease":[ "7", "8" ] },
    { "operatingsystem":"CentOS", "operatingsystemrelease":[ "7", "8" ] },
    { "operatingsystem":"Fedora", "operatingsystemrelease": [ "33", "34" ] },
  ],
  "dependencies": [
    {"name":"puppet/extlib", "version_requirement":">=4.0.0"},
    {"name":"puppet/selinux", "version_requirement":">=3.0.0 <= 4.0.0"},
    {"name":"puppetlabs/stdlib", "version_requirement":">=2.2.0 <= 5.2.0"},
    {"name":"puppetlabs/mysql", "version_requirement":">= 9.1.0 < 10.0.0"},
    {"name":"puppetlabs/postgresql","version_requirement":">= 6.1.0 < 7.0.0"},
    {"name":"puppetlabs/sqlite", "version_requirement":">=0.0.1 <= 0.1.0"},
    {"name":"puppetlabs/firewall", "version_requirement":">= 2.0.0 < 3.0.0"}
  ],
  "requirements": [
    {"name": "puppet", "version_requirement": ">= 4.0.0 < 7.0.0"}
  ]
}
