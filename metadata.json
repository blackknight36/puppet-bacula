{
  "name": "mdct-bacula",
  "version": "0.6.2",
  "author": "Dart Container and Michael Watters",
  "summary": "This module is based on rharrison10-bacula",
  "license": " Apache-2.0",
  "source": "git://mdct-pagure.dartcontainer.com/mdct/puppet-bacula.git",
  "project_page": "https://mdct-pagure.dartcontainer.com/mdct/puppet-bacula",
  "issues_url": "https://mdct-pagure.dartcontainer.com/mdct/puppet-bacula/issues",
