FileSet {
  Name = "<%= @name %>"
  EnableVSS = <%= @enable_vss ? 'yes':'no' %>
  Include {
    Options {
<% @options.each do |key,value| -%>
      <%= key %> = <%= value %>
<% end -%>
    }
<% if @include_files -%>
<% @include_files.each do |file_include| -%>
    File = <%= file_include %>
<% end -%>
<% end -%>
  }
<% if @exclude_files -%>
  Exclude {
<% @exclude_files.each do |file_exclude| -%>
    File = <%= file_exclude %>
<% end -%>
  }
<% end -%>
}
